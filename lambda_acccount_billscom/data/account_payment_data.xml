<?xml version="1.0" encoding='UTF-8'?>
<odoo>

    <record model="ir.cron" id="billscom_account_move_bills_cron">
        <field name="name">Bills.com: Check bills and payments on bills.com</field>
        <field name="model_id" ref="account.model_account_payment"/>
        <field name="state">code</field>
        <field name="code"><![CDATA[
model._check_bills_payment()
]]></field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" eval="(datetime.now() + timedelta(minutes=7)).strftime('%Y-%m-%d %H:%M:%S')"/>
    </record>

</odoo>